import java.util.Scanner;

public class main {

    public static void main(String[] args) {
        Scanner reader = new Scanner(System.in);
        int a1 = 1, a2 = 2, a3, i, a;
        System.out.print("Fibonacci serisi için bir sayı girin:");
        a = reader.nextInt();
        System.out.print(a1 + " " + a2);

        for (i = 2; i < a; i++) {
            a3 = a1 + a2;
            System.out.print(" " + a3);
            a1 = a2;
            a2 = a3;
        }
    }

}
